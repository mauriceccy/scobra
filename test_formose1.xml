          <speciesReference species="M_C4ald" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="M_C4fur" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="R_Split4" name="C4 back to two C2" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_C4ald" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="M_C2" stoichiometry="2" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="R_Form1" name="Formaldehyde input" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfProducts>
          <speciesReference species="M_C1" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="R_Out3k" name="Hydroxyacetone product" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_C3ket" stoichiometry="1" constant="true"/>
        </listOfReactants>
      </reaction>
      <reaction id="R_Out4k" name="C4 ketone product" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_C4ket" stoichiometry="1" constant="true"/>
        </listOfReactants>
      </reaction>
      <reaction id="R_OutPdt" name="C4 Ring Product Output" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_C4fur" stoichiometry="1" constant="true"/>
        </listOfReactants>
      </reaction>
      <reaction id="R_Out2" name="Glycolaldehyde Pdt" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_C2" stoichiometry="1" constant="true"/>
        </listOfReactants>
      </reaction>
      <reaction id="R_Out3a" name="Glyceraldehyde Pdt" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_C3ald" stoichiometry="1" constant="true"/>
        </listOfReactants>
      </reaction>
      <reaction id="R_Out4a" name="C4 aldehyde Pdt" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_C4ald" stoichiometry="1" constant="true"/>
        </listOfReactants>
      </reaction>
    </listOfReactions>
    <fbc:listOfObjectives fbc:activeObjective="obj">
      <fbc:objective fbc:id="obj" fbc:type="maximize"/>
    </fbc:listOfObjectives>
  </model>
</sbml>
