        <listOfProducts>
          <speciesReference species="M_C4ald" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="R_Isom4" name="C4 isomerization" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
          <speciesReference species="M_C4ald" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="M_C4ket" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="R_Ring4" name="C4 ring closure" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
