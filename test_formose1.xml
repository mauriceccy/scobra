        </listOfReactants>
        <listOfProducts>
          <speciesReference species="M_C3ket" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="R_Form4" name="C4 formation" reversible="false" fast="false" fbc:lowerFluxBound="cobra_0_bound" fbc:upperFluxBound="cobra_default_ub">
        <listOfReactants>
